
<% include ./partials/header %>


<div class="container">
  <div class="page-header text-center">
    
        <h1><span class="fa fa-anchor"></span> Your Orders</h1>
    </div>


  <div class="well">
   
   <table class="table table-hover" id="myTable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>User name</th>
      <th>Orders </th>
    </tr>
  </thead>

  <% users.forEach(function(file,i){ %>
  <tbody>
    <tr>
      <td><%= file.local.firstName %></td>
      <td><%= file.local.email %></td>
      <td><%= file.local.username %></td>
      <td><a href="/orderedList/<%= file._id %>" >ORDERS</a></td>
      
      
    </tr>
  <% }) %>
  </tbody>
</table>


</div>


</div>

<% include ./partials/footer %>


