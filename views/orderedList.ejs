
<% include ./partials/header %>


<div class="container">

  <div class="well">

   <table class="table table-hover" id="myTable">
  <thead>
    <tr>
      <th>Size</th>
      <th>Quantity</th>
      <th>Delivery Address</th>
      <th>Associated Email</th>
      <th>Associated Contact</th>
      <th>Notes</th>
      <th>Total</th>
      <th></th>
    </tr>
  </thead>

  <% reqproduct.forEach(function(file,i){ %>
  <tbody>
    <tr>
      <td><%= file.productSize %></td>
      <td><%= file.productQuantity %></td>
      <td><%= file.deliveryAddress %></td>
      <td><%= file.email %></td>
      <td><%= file.contact %></td>
      <td><%= file.notes %></td>
      <td><%= file.total %> </td>
      <td><img src="/uploads/<%= file.image %>" width="200" height="200"></td>
      
      
    </tr>
  <% }) %>
  </tbody>
</table>


</div>


</div>

<% include ./partials/footer %>


