<!-- Add Google Maps -->
<script>
function myMap()
{
  myCenter=new google.maps.LatLng(27.6196189, 85.538637);
  var mapOptions= {
    center:myCenter,
    zoom:12, scrollwheel: false, draggable: false,
    mapTypeId:google.maps.MapTypeId.ROADMAP
  };
  var map=new google.maps.Map(document.getElementById("googleMap"),mapOptions);

  var marker = new google.maps.Marker({
    position: myCenter,
  });
  marker.setMap(map);
}

</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCZr5Qu0F1Z_82NZMuap3kXxHjvtNrrQVU&callback=myMap"></script>
